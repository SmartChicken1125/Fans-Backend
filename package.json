	"type": "module",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "node dist/index.js",
		"debug": "yarn build && node --inspect-brk dist/index.js",
		"cli": "SERVICES=cli node dist/index.js",
		"dev": "nodemon",
		"format": "yarn format-path .",
		"format-path": "prettier --write --ignore-path .gitignore --ignore-path .prettierignore --plugin=prettier-plugin-prisma",
		"test": "mocha --exit",
		"prisma:seed": "yarn build && NODE_OPTIONS='-r dotenv/config' node dist/prisma/seed.js",
		"prisma:convert": "yarn build && node dist/prisma/convert.js",
		"prisma:migrate": "yarn prisma migrate deploy",
		"lint": "tsc --noEmit -p tsconfig.json && eslint --ignore-path .gitignore --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx ."
